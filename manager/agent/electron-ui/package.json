{
  "name": "electron-ui",
  "version": "1.0.0",
  "description": "Electron-based UI application for managing services",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.electron.ui",
    "files": [
      "**/*",
      ".env"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "axios": "^1.7.9",
    "crypto": "^1.0.1",
    "electron-packager": "^17.1.2",
    "plotly.js-dist": "^2.35.3"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^25.1.8"
  }
}
